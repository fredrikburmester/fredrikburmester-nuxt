{
  // https://v3.nuxtjs.org/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["esnext","dom"],
    "module": "esnext",
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "target": "esnext",
    "types": ["node","@nuxt/types","@pinia/nuxt"],
    "outDir": "dist"

  },
  "exclude": ["node_modules"]
}
